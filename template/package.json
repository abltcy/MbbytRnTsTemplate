  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim": {
        "binaryPath": "ios/Build/Products/Debug-iphonesimulator/ProjectName.app",
        "type": "ios.simulator",
        "name": "iPhone 13"
      }
    },
    "test-runner": "jest",
    "specs": ""
