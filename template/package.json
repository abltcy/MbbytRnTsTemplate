    "@stripe/stripe-react-native": "^0.2.3",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "native-base": "^3.2.1",
    "react": "17.0.2",
    "react-native": "0.66.2",
    "react-native-appstate-hook": "^1.0.6",
    "react-native-axios": "^0.17.1",
    "react-native-bootsplash": "^4.0.0",
    "react-native-code-push": "^7.0.4",
    "react-native-config": "^1.4.5",
