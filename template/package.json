{
  "name": "projectname",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=stagingDebug",
    "android:staging": "ENVFILE=.env.staging react-native run-android --variant=stagingDebug && cd android && ./gradlew assembleStagingDebug",
    "android:staging-release": "ENVFILE=.env.staging react-native run-android --variant=stagingRelease && cd android && ./gradlew assembleStagingRelease",
    "run-ios:staging": "react-native run-ios --scheme ProjectName.Staging",
    "run-ios:staging6s": "react-native run-ios --scheme ProjectName.Staging --simulator='iPhone 6s'",
    "run-ios:production": "react-native run-ios --scheme ProjectName",
    "ios": "react-native run-ios",
    "start": "react-native start  --reset-cache",
    "test": "jest",
    "e2e:build-ios": "detox build --configuration ios",
    "e2e:run-ios": "detox test --configuration ios",
    "e2e:build-android": "detox build --configuration android",
    "e2e:run-android": "detox test --configuration android",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "fullinstall": "yarn install && cd ios && arch -x86_64 pod install && cd .."
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.12.0",
    "@react-native-async-storage/async-storage": "^1.15.11",
    "@react-native-community/datetimepicker": "^3.5.2",
